{
    "extends": "eslint:recommended",
    "rules": {
      "semi": ["warn", "never"],
      "quotes": ["warn", "single"],
      "no-console": ["off"],
      "no-undef": ["off"]
    },
    "parserOptions": {
      "ecmaVersion": 9
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "amd": true
    },
    "globals": {
      "$": true,
      "require": true,
      "process": true
    },
    "root": true
  }
  